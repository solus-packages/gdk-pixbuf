<?xml version="1.0" ?>
<!DOCTYPE PISI SYSTEM "https://solus-project.com/standard/pisi-spec.dtd">
<PISI>
    <Source>
        <Name>gdk-pixbuf</Name>
        <Homepage>http://www.gnome.org</Homepage>
        <Packager>
            <Name>Ikey Doherty</Name>
            <Email>ikey@solusos.com</Email>
        </Packager>
        <Summary>GNOME toolkit for image loading and pixel buffer manipulation</Summary>
        <Description>The Gdk Pixbuf is a toolkit for image loading and pixel buffer manipulation. It is used by GTK+ 2 and GTK+ 3 to load and manipulate images.</Description>
        <License>GPLv3+</License>
        <Archive sha1sum="40482f165f3fc587e54a08bb9212aeca97e2d07d" type="tarxz">http://ftp.gnome.org/pub/gnome/sources/gdk-pixbuf/2.32/gdk-pixbuf-2.32.1.tar.xz</Archive>

        <BuildDependencies>
            <Dependency type="pkgconfig">libtiff-4</Dependency>
            <Dependency type="pkgconfig32">libtiff-4</Dependency>
            <Dependency>libjpeg-turbo-devel</Dependency>
            <Dependency>libjpeg-turbo-32bit-devel</Dependency>
            <Dependency type="pkgconfig">libpng15</Dependency>
            <Dependency type="pkgconfig32">libpng15</Dependency>
            <Dependency type="pkgconfig">libpcre</Dependency>
            <Dependency type="pkgconfig32">libpcre</Dependency>
            <Dependency type="pkgconfig">gobject-2.0</Dependency>
            <Dependency type="pkgconfig32">gobject-2.0</Dependency>
            <Dependency type="pkgconfig">x11</Dependency>
            <Dependency type="pkgconfig32">x11</Dependency>
            <Dependency>libgcc-32bit</Dependency>
            <Dependency>glibc-32bit-devel</Dependency>
        </BuildDependencies>
    </Source>

    <Package>
        <Name>gdk-pixbuf</Name>
        <Files>
            <Path fileType="executable">/usr/bin</Path>
            <Path fileType="library">/usr/lib/gdk-pixbuf-2.0</Path>
            <Path fileType="library">/usr/lib/lib*.so.*</Path>
            <Path fileType="data">/usr/share/gir-1.0</Path>
            <Path fileType="data">/usr/lib/girepository-1.0</Path>
            <Path fileType="locale">/usr/share/locale</Path>
            <Path fileType="man">/usr/share/man</Path>
        </Files>
        <Provides>
            <COMAR script="package.py">System.Package</COMAR>
            <COMAR script="packageHandler.py">System.PackageHandler</COMAR>
        </Provides>
    </Package>

    <Package>
        <Name>gdk-pixbuf-32bit</Name>
        <BuildType>emul32</BuildType>
        <PartOf>emul32</PartOf>
        <Summary>32bit files for gdk-pixbuf</Summary>
        <Description>32bit files for gdk-pixbuf</Description>
        <Files>
            <Path fileType="library">/usr/lib32/gdk-pixbuf-2.0</Path>
            <Path fileType="data">/usr/lib32/girepository-1.0</Path>
            <Path fileType="library">/usr/lib32/lib*.so.*</Path>
        </Files>
        <RuntimeDependencies>
            <Dependency>gdk-pixbuf</Dependency>
        </RuntimeDependencies>
    </Package>

    <Package>
        <Name>gdk-pixbuf-devel</Name>
        <Summary>Development files for gdk-pixbuf</Summary>
        <Description>Development files for gdk-pixbuf</Description>
        <Files>
            <Path fileType="header">/usr/include</Path>
            <Path fileType="library">/usr/lib/lib*.so</Path>
            <Path fileType="data">/usr/lib/pkgconfig</Path>
        </Files>
        <RuntimeDependencies>
            <Dependency release="current">gdk-pixbuf</Dependency>
        </RuntimeDependencies>
    </Package>

    <Package>
        <Name>gdk-pixbuf-32bit-devel</Name>
        <BuildType>emul32</BuildType>
        <PartOf>emul32</PartOf>
        <Summary>Development files for gdk-pixbuf-32bit</Summary>
        <Description>Development files for gdk-pixbuf-32bit</Description>
        <Files>
            <Path fileType="library">/usr/lib32/lib*.so</Path>
            <Path fileType="data">/usr/lib32/pkgconfig</Path>
        </Files>
        <RuntimeDependencies>
            <Dependency release="current">gdk-pixbuf-devel</Dependency>
            <Dependency release="current">gdk-pixbuf-32bit</Dependency>
        </RuntimeDependencies>
    </Package>

    <Package>
        <Name>gdk-pixbuf-docs</Name>
        <Summary>Documentation for gdk-pixbuf</Summary>
        <Description>Documentation for gdk-pixbuf</Description>
        <Files>
            <Path fileType="doc">/usr/share/doc</Path>
            <Path fileType="doc">/usr/share/gtk-doc</Path>
        </Files>
        <RuntimeDependencies>
            <Dependency release="current">gdk-pixbuf-devel</Dependency>
        </RuntimeDependencies>
    </Package>



